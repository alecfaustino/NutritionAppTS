  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "rootDir": "./server",
    "outDir": "./dist-server",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "strict": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": ["server"]
}
